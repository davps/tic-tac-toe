    "@storybook/addons": "^3.4.8",
    "@storybook/react": "^3.4.8",
    "@storybook/storybook-deployer": "^2.3.0",
    "@types/react": "^16.4.1",
    "@types/react-dom": "^16.0.6",
    "babel-core": "^6.26.3",
    "babel-runtime": "^6.26.0",
    "coveralls": "^3.0.1",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-redux": "^0.2.1",
